{
    "name": "yanev/laraadmin",
    "version": "2.0.0",
    "type": "project",
    "description": "LaraAdmin is a Open source Laravel Admin Panel / CMS which can be used as Admin Backend, Data Management Tool or CRM boilerplate for Laravel with features like CRUD Generation, Module Manager, Media, Menus, Backups and much more",
    "keywords": [
        "php",
        "laravel",
        "crm",
        "admin",
        "cms",
        "administrator"
    ],
    "license": "MIT",
    "homepage": "http://laraadmin.com",
    "support": {
        "issues": "https://github.com/yanev/laraadmin/issues",
        "source": "https://github.com/yanev/laraadmin"
    },
    "require": {
        "php": "^8.3",
        "doctrine/dbal": "^3.5",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^11.0.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.5.2",
        "spatie/laravel-html": "^3.9.0",
        "spatie/laravel-backup": "^8.8.0",
        "yajra/laravel-datatables-oracle": "^11.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.14",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1.0",
        "phpunit/phpunit": "^11.0.0"
    },
    "authors": [
        {
            "name": "Ganesh Bhosale",
            "email": "ganesh@dwij.in"
        },
        {
            "name": "Ganesh Khade",
            "email": "moraya.re@gmail.com"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/laraadmin/entrust.git",
            "type": "git"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "sniff": [
            "./vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots"
        ],
        "lint": [
            "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}